
fun Main(args: string[*]): i32 {
    result := 1;
    result += 2;
    result *= 3;
    result /= 4;
    result -= 5;
    result %= 6;
    result <<= 7;
    result >>= 8;
    result &= 9;
    result ^= 10;
    result |= 11;
    result = -result;
    result = +result;
    result = ~result;
    result = !result;
    result = 0;
    return result;
}